{
  "openapi": "3.0.1",
  "info": {
    "title": "SAN-API",
    "version": "1.0"
  },
  "paths": {
    "/login": {
      "post": {
        "tags": [
          "Authentification"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SeConLoginHelper"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SeConLoginHelper"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SeConLoginHelper"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/sigin": {
      "post": {
        "tags": [
          "Authentification"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "modelForm": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/StringStringValuesKeyValuePair"
                    }
                  }
                }
              },
              "encoding": {
                "modelForm": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/me": {
      "get": {
        "tags": [
          "Authentification"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/logout": {
      "post": {
        "tags": [
          "Authentification"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/recDonne": {
      "get": {
        "tags": [
          "Teste"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/recFormulaire": {
      "get": {
        "tags": [
          "Teste"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/mesProjets": {
      "get": {
        "tags": [
          "Teste"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/WeatherForecast": {
      "get": {
        "tags": [
          "WeatherForecast"
        ],
        "operationId": "GetWeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SeConLoginHelper": {
        "type": "object",
        "properties": {
          "identifier": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "remember": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "StringStringValuesKeyValuePair": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "nullable": true
          },
          "value": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}